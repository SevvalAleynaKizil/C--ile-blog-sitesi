
<div class="container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>#</td>
                <td>Etkilesim</td>
                <td>Ip Adresi</td>
                <td>İşlemler</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                     <td>@(item.Status==true?"Beğendi" : "Beğenmedi")</td>
                      <td>@item.IpAddress</td>
                       <td>
                           <a href="/admin/Etkilesim/Delete/@item.Id" class="btn btn-danger">Sil</a>
                       </td>
                </tr>
                
            }


        </tbody>

    </table>
</div>